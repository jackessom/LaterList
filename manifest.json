{
	 "manifest_version": 2,
	
	"name": "LaterList",
	"description": "Temporary bookmark system for when you are just fed up of all those open tabs but don't like the commitment of bookmarks.",
	"version": "0.9",
	
	"permissions": [
		"tabs",
		"storage",
		"contextMenus"
	],
	
	"icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
	},
	
	"background": {
    "scripts": ["background.js"],
    "persistent": false
	},	
	
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	}
}